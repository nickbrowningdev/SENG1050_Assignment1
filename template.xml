  <!ELEMENT goods_pic (#PCDATA)>
]>

<template>
	<farm>
		<goods_type></goods_type>
		<farm_name></farm_name>
		<farm_logo></farm_logo>
		<farm_picture></farm_picture>
		<farm_description></farm_description>
		<url></url>
		<address></address>
		<phone></phone>
		<delivery>
				<min_price></min_price>
				<deli_fee></deli_fee>
		</delivery>
		<goods>
				<goods_name></goods_name>
				<goods_pic></goods_pic>
		</goods>
	</farm>
</template>	